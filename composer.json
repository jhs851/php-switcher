{
    "name": "hyungseok/phpswitcher",
    "description": "Switch the php version with Valet using Homebrew.",
    "keywords": ["php", "switch", "valet", "homebrew"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hyungseok Jeong",
            "email": "jhs851767@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hyungseok\\PHPSwitcher\\Console\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6",
        "symfony/console": "^2.8.5",
        "symfony/process": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "bin": [
        "switcher"
    ]
}
